<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.exemplosharedpreferences"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.exemplosharedpreferences.MainActivity"
            android:label="MainActivity" >
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="br.exemplosharedpreferences.SecondActivity"
            android:label="SecondActivity" >
		</activity>
		
        <activity
            android:name="br.exemplosharedpreferences.LoginActivity"
            android:label="LoginActivity"
            android:noHistory="true" >
		</activity>
		
        <activity
            android:name="br.exemplosharedpreferences.ProfileActivity"
            android:label="ProfileActivity" >
		</activity>
    </application>

</manifest>
